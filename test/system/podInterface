podInterface_p
{
    type            podInterface;
    // Where to load it from (if not already in solver)
    functionObjectLibs ("libPODAnalysis.so");

    debug           false;

    outputControl   outputTime; //timeStep;

    outputInterval  1;

    fieldName p;

    resultName pMode;

    timeStart 0.1;

    timeEnd  0.2;

    chunkNumber 3;

    writePrecision 16;
}

